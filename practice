"""
# 4344

case = int(input())
result = []
count = 0

for i in range(case):
    t_list = list(map(int, input().split()))
    avg = (sum(t_list) - t_list[0]) / t_list[0]
    for j in range(1, t_list[0] + 1):
        if t_list[j] > avg:
            count += 1
    print(str("%0.3f" % float(count/t_list[0]*100)) + "%")
    count = 0
