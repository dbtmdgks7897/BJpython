"""
# 4673

def d(n):
    a = 0
    for i in str(n):
        a += int(i)
    return int(n) + a


result = set(range(1,10001))
minus = set()
for i in range(1, 10001):
    minus.add(d(i))
result = result - minus

for i in sorted(result):
    print(i)
